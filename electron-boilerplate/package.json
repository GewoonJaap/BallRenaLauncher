{
  "name": "ballrena-launcher",
  "productName": "BallRena Launcher",
  "description": "BallRena game launcher and updater | Made with ElectronJS",
  "version": "1.6.0",
  "private": true,
  "author": "BallRena",
  "copyright": "Â© 2019, BallRena",
  "homepage": "https://www.ballrena.ml",
  "main": "app/main.js",
  "build": {
    "appId": "nl.ballrena.electronjslauncher",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json",
      "app/assets/config/config.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": [
      {
        "provider": "github",
        "repo": "BallRenaLauncherDownloads",
        "owner": "GewoonJaap"
      }
    ]
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder",
    "release2": "electron-builder",
    "publish": "electron-builder --publish always"
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "app": "^0.1.0",
    "electron-dl": "^1.13.0",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^3.0.1",
    "electron-store": "^2.0.0",
    "electron-titlebar-windows": "^3.0.0",
    "electron-unhandled": "^2.1.0",
    "electron-updater": "^4.0.6",
    "electron-util": "^0.11.0",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.1",
    "fs-jetpack": "^2.1.0",
    "install": "^0.12.2",
    "jquery": "^3.3.1",
    "mkdirp": "^0.5.1",
    "node-emoji": "^1.8.1",
    "npm": "^6.8.0",
    "open": "0.0.5",
    "opn": "^5.4.0",
    "popups": "^1.1.3",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "superagent": "^4.1.0",
    "systeminformation": "^4.0.10",
    "unzip": "^0.1.11",
    "update-electron-app": "^1.3.0",
    "xel": "0.0.193"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "css-loader": "^1.0.0",
    "electron": "^4.0.4",
    "electron-builder": "^20.15.1",
    "electron-mocha": "^6.0.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
